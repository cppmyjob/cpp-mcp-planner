{
  "name": "@mcp-planner/web-server",
  "version": "1.0.0",
  "description": "NestJS HTTP server for MCP Planning Web Dashboard",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node dist/main.js",
    "start:dev": "tsx watch src/main.ts"
  },
  "dependencies": {
    "@mcp-planner/core": "*",
    "@nestjs/common": "^10.4.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/swagger": "^8.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "joi": "^17.13.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.0",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}
